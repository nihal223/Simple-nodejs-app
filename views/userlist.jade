extends layout

block content
	h1.
		User List
	ul
		each user, i in userlist
			li
			p Name is #{user.name}
			p Mobilenumber = #{user.mobile_number}
			p email id is #{user.email}
	.col-md-6.col-md-offset-3
		if (errors)
			.alert.alert-danger
			p= message
			ul
				each error in errors
					li= error.msg

			
				
